/*整个页面的样式*/
.container{
  display: flex;
  flex-direction: column;
  justify-content: center;
}

/*导航图标*/
.icon{
  display: flex;
  justify-content: center;
  width: 99%;
  margin-top: 40rpx;
  flex-wrap: wrap;
}
.combination{
  width: 32%;
  height: 290rpx;
  text-align: center;
  font-size: 30rpx;
  margin-bottom: 50rpx;
}
.icon view image{
  width: 220rpx;
	height: 220rpx;
	padding: 11rpx;
  border-radius: 40rpx;
}

/*分界线*/
.bottom_material_line{
  background-color: #f6f6f6;
  height: 8rpx;
  margin-bottom: 20rpx;
}

/*没有更多*/
.no_more{
  color: #5b5a5a;
  text-align: center;
  font-size: 28rpx;
  margin-bottom: 20rpx;
}