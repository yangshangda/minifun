/*整个页面的样式*/
.container{
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
}
.title{
  text-align: center;
  margin-top: 20rpx;
}
.big-v{
  display: flex;
  justify-content: center;
  width: 99%;
  margin-top: 40rpx;
  flex-wrap: wrap;
  margin-bottom: 80rpx;
}
image{
  max-width: 550rpx;
  max-height: 550rpx;
}
block{
  width: 80%;
  height: 80%;
}
.input-v{
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
  margin-top: 20rpx;
  width: 90%;

}

label{
  display: flex;
  flex-direction:column ;
  flex-wrap:wrap ;
}
text{
  color:rgb(105, 105, 104);
  margin-right: 10rpx;
}

input{
    border:  1px solid rgb(206, 200, 190);
    width: 60%;
}

button{
  margin-top: 50rpx;
  width: 70%;
  background-color: #EDB34F;
  color: white;
}

